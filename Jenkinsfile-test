pipeline {
    agent any
        stages {
        stage('Build') {
            steps {
                sh 'mvn -B package'
            }
        }
        stage("Create Docker image") {
            steps {
                script {
                    sh "C:/ProgramData/Jenkins/.jenkins/workspace/trabajo-final-devops/target/ \
                        docker build -t imagen-artefacto email2-0.0.1-SNAPSHOT.jar"
                }
            }
        }
    }
}
