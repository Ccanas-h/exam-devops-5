pipeline {
    agent any
        stages {
        stage('Build') {
            steps {
                sh 'mvn -B package'
            }
        }
                stage("Start Docker") {
            steps {
                script {
                    sh 'service docker start'
                }
            }
        }
        stage("Create Docker image") {
            steps {
                script {
                    sh "/var/jenkins_home/workspace/Jenkins-SonarQ-Nexus-GithubWebhook-Ngrok-Slack-Pipeline/target/ \
                        sudo docker build -t imagen-artefacto ."
                }
            }
        }
    }
}
