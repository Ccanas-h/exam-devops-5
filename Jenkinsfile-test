pipeline {
    agent any
        stages {
        stage('Build') {
            steps {
                sh 'mvn -B package'
            }
        }
                stage("Start Docker") {
            steps {
                script {
                    sh 'sudo service docker start'
                }
            }
        }
                stage("Create Docker image") {
            steps {
                script {
                    sh 'docker build -t my-docker-image .'
                }
            }
        }
    }
}
